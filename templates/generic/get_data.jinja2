{% from 'components.jinja2' import pbs_head, singularity -%}
{{ pbs_head(log_dir=WORKING_DIR) }}

source {{ XNAT_PBS_SETUP_SCRIPT_PATH }}

{{ singularity(
        container=SINGULARITY_CONTAINER_PIPELINES_PATH,
