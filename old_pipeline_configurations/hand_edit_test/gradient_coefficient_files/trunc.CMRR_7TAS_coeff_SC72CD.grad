106 A( 7, 1)                  0.0388958000                            x
107 A( 7, 3)                  0.0217300000                            x
108 A( 9, 1)                 -0.0095427200                            x
109 A( 9, 3)                 -0.0114753000                            x
201 B( 3, 1)                 -0.1577050000                            y
202 B( 3, 3)                  0.0043569500                            y
